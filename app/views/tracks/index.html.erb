<div class="row">
  <div class="col-lg-12">
    <div class="wrapper wrapper-content animated fadeInUp">

        <div class="ibox">
          <div class="ibox-title">
              <h5>Tracks</h5>
              <div class="ibox-tools">
                  <a href="" class="btn btn-primary btn-xs">Create new Tracks</a>
              </div>
          </div>
          <div class="ibox-content">
              <div class="project-list">

                 <table class="table table-hover">
                   <thead>
                       <tr>
                         <th>Name</th>
                         <th>Progress</th>
                         <th>Due Date</th>
                         <th></th>
                       </tr>
                   </thead>
                   <tbody>

                       <tr>
                         <td class="Track-title">
                             <a href="">Web development</a>
                             <br />
                             <small>Created 14.08.2014</small>
                         </td>
                         <td class="project-completion">
                             <small>Completion with: 28%</small>
                             <div class="progress progress-mini">
                                 <div style="width: 28%;" class="progress-bar"></div>
                             </div>
                         </td>
                         <td class="resource-due-date">
                           <i class="fa fa-calendar"></i> 14.08.2014
                         </td>
                         <td class="project-actions">
                           <% track.goals.each do |g| %>
                              <%= link_to g.name, track_goal_path(g) %>
                           <% end %>

                             <%= link_to "+", new_track_goal_path, track %>
                             <a href="#" class="btn btn-white btn-sm"><i class="fa fa-pencil"></i> Edit </a>
                             <a href="#" class="btn btn-white btn-sm"><i class="fa fa-trash"></i> Delete </a>
                         </td>
                       </tr>
                   </tbody>
                 </table>
               </div>
            </div>
        </div>
      </div>
    </div>
  </div>
